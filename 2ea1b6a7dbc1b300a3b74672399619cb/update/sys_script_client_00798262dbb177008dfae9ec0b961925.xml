   }

	var ajax = new GlideAjax('x_brb_work_order.WorkOrderUtils');
	ajax.addParam('sysparm_name', 'getUserInfo');
	ajax.addParam('sysparm_sys_id', newValue);
	ajax.getXMLAnswer(handleUserInfo);
	
